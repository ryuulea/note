Public Sub DebugDeleteLoop10()

    Dim wsMail As Worksheet
    Dim lastMail As Long, i As Long
    Dim fnum As Integer, filePath As String
    
    Set wsMail = ThisWorkbook.Sheets("メール本文テンプレート情報")
    lastMail = wsMail.Cells(wsMail.Rows.Count, "A").End(xlUp).Row
    
    filePath = ThisWorkbook.Path & "\debug_delete10.txt"
    fnum = FreeFile
    Open filePath For Output As #fnum
    
    For i = 11 To lastMail
        Print #fnum, "行:" & i & _
            " Value=[" & CStr(wsMail.Cells(i, 4).Value) & "]" & _
            " Value2=[" & CStr(wsMail.Cells(i, 4).Value2) & "]" & _
            " Text=[" & wsMail.Cells(i, 4).Text & "]" & _
            " Formula=[" & wsMail.Cells(i, 4).Formula & "]"
    Next i
    
    Close #fnum
    MsgBox "Debug10 完了: " & filePath

End Sub
