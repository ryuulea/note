Public Sub DebugDeleteLoop5()

    Dim wsMail As Worksheet
    Dim lastMail As Long, i As Long
    Dim fnum As Integer, filePath As String
    Dim val As String
    
    ' 改成索引方式，避免名字错
    Set wsMail = ThisWorkbook.Sheets(1)
    
    lastMail = wsMail.Cells(wsMail.Rows.Count, "A").End(xlUp).Row
    
    filePath = ThisWorkbook.Path & "\debug_delete5.txt"
    fnum = FreeFile
    Open filePath For Output As #fnum
    
    For i = 1 To lastMail
        val = CStr(wsMail.Cells(i, 4).Value2)   ' 用 Value2 + 强转字符串
        Print #fnum, "行:" & i & " D列=[" & val & "]"
    Next i
    
    Close #fnum
    MsgBox "Debug5 完了: " & filePath

End Sub
