Public Sub DebugDeleteRow11()

    Dim wsMail As Worksheet
    Dim filePath As String, fnum As Integer
    Dim val As String
    
    Set wsMail = ThisWorkbook.Sheets("メール本文テンプレート情報")
    
    val = CStr(wsMail.Cells(11, 4).Value2)  ' 第11行 D列
    
    filePath = ThisWorkbook.Path & "\debug_row11.txt"
    fnum = FreeFile
    Open filePath For Output As #fnum
    Print #fnum, "第11行D列=[" & val & "]"
    Close #fnum
    
    MsgBox "DebugRow11 完了: " & filePath

End Sub
