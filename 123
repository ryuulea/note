Public Sub DebugDeleteLoop7()

    Dim wsMail As Worksheet
    Dim lastMail As Long, i As Long
    Dim fnum As Integer, filePath As String
    Dim val As String
    
    Set wsMail = ThisWorkbook.Sheets("メール本文テンプレート情報")
    
    lastMail = wsMail.Cells(wsMail.Rows.Count, "A").End(xlUp).Row
    
    filePath = ThisWorkbook.Path & "\debug_delete7.txt"
    fnum = FreeFile
    Open filePath For Output As #fnum
    
    For i = 1 To lastMail
        val = CStr(wsMail.Cells(i, 2).Value2)   ' B列（扱い）
        Print #fnum, "行:" & i & " B列=[" & val & "]"
    Next i
    
    Close #fnum
    MsgBox "Debug7 完了: " & filePath

End Sub
